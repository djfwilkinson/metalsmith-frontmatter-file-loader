    "type": "git",
    "url": "git+https://github.com/djfwilkinson/metalsmith-frontmatter-file-loader.git"
  },
  "keywords": [
    "metalsmith",
    "frontmatter"
  ],
  "author": "Daniel Wilkinson <opensource@djfw.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djfwilkinson/metalsmith-frontmatter-file-loader/issues"
  },
  "homepage": "https://github.com/djfwilkinson/metalsmith-frontmatter-file-loader#readme",
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^7.0.1",
